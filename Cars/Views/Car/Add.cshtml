@model Cars.Models.AddCarViewModel

@{
}
<h1>Add Car</h1>
<form method="post" action="Add" class="mt-5">
    <div class="mb-3">
        <label for="" class="form-label">Model</label>
        <input type="text" class="form-control" asp-for="Modell">
       
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Brand</label>
        <input type="text" class="form-control" asp-for="Brand">

    </div>
    <div class="mb-3">
        <label for="" class="form-label">Price</label>
        <input type="number" class="form-control" asp-for="Price">

    </div>
    <div class="mb-3">
        <label for="" class="form-label">Manufacture Date</label>
        <input type="date" class="form-control" asp-for="ManufactureDate">

    </div>
    <div class="mb-3">
        <label for="" class="form-label">Category</label>
        <input type="text" class="form-control" asp-for="category">

    </div>

  
    <button type="submit" class="btn btn-primary">Submit</button>
</form>